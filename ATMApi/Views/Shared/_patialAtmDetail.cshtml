@model ATM_FollowUp.Models.ATM

<center><h2><b>Details</b></h2></center>
<center><h3>ATM @Model.id</h3></center>
<hr />
<form method="post" action="/atm/edit">
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Id</label>
        <div class="col-sm-10">
            <input type="number" class="form-control" id="id" name="id" disabled value="@Model.id" />
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Name</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="name" name="name" disabled value="@Model.name" />
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Address</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="address" name="address" disabled value="@Model.address" />
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Balance</label>
        <div class="col-sm-10">
            <input type="number" id="balance" class="form-control" name="balance" disabled value="@Model.balance" />
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Longtude</label>
        <div class="col-sm-10">
            <input type="text" id="longtude" class="form-control" name="longtude" disabled value="@Model.longtude" />
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Latitude</label>
        <div class="col-sm-10">
            <input type="text" id="latitude" class="form-control" name="latitude" disabled value="@Model.latitude" />
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Trunsaction number</label>
        <div class="col-sm-10">
            <input type="text" id="latitude" class="form-control" name="T_N" disabled value="@Model.T_N" />
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Trunsaction last hour</label>
        <div class="col-sm-10">
            <input type="text" id="latitude" class="form-control" name="T_N_last_hour" disabled value="@Model.T_N_last_hour" />
        </div>
    </div>
</form>

<p>
    @Html.ActionLink("Back to List", "Index")
</p>

